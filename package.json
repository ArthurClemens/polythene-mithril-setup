{
  "name": "polythene-mithril-setup",
  "description": "Polythene for Mithril setup example",
  "version": "1.3.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/polythene-mithril-setup.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
