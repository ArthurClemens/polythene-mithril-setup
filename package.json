    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
